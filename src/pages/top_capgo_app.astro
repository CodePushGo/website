---
import Layout from '@/layouts/Layout.astro'
import VueTopcodepushgoapp from '@/vue-pages/top_capgo_app.vue'
import * as m from '../paraglide/messages.js'

const title = [Astro.locals.runtimeConfig.public.brand, m.top_codepushgo_apps_title()].join(' | ')
const description = m.top_codepushgo_apps()

const content = { title, description }
---

<Layout content={content}><VueTopcodepushgoapp locale={Astro.locals.locale} client:load /></Layout>
