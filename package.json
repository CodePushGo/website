{
  "type": "module",
  "name": "codepushgo-website",
  "scripts": {
    "astro": "astro",
    "dev": "tsx scripts/fix_markdown_languages.tsx && astro dev",
    "start": "astro dev",
    "build": "time paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && time tsx scripts/fix_markdown_languages.tsx && time astro sync && astro build && time tsx ./scripts/repair_sitemap.tsx",
    "preview": "astro preview",
    "types": "npx --yes supabase gen types typescript --project-id=xvwzpoazmxkqosrdewyv > src/services/supabase.types.ts",
    "fmt": "prettier --write '**/*' --ignore-unknown",
    "generate:locale:translations": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "generate:blog:translations": "tsx scripts/generate_blog_translations.tsx",
    "generate:blog:translations:new": "tsx scripts/translate_new_blogs.tsx",
    "generate:docs:translations": "tsx scripts/generate_docs_translations.tsx",
    "generate:plugin:translations": "tsx scripts/generate_plugin_translations.tsx",
    "setup:new:locale": "tsx scripts/setup_new_locale.tsx",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "clean:unused": "npx tsx scripts/clean_not_used.tsx"
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "0.32.2",
    "@astrojs/starlight-tailwind": "^4.0.1",
    "@dotenvx/dotenvx": "^1.38.4",
    "@inlang/paraglide-astro": "^0.3.5",
    "@langchain/community": "^0.3.34",
    "@orama/wc-components": "^0.7.0",
    "@langchain/core": "^0.3.42",
    "@langchain/openai": "^0.4.4",
    "fast-xml-parser": "^5.2.1",
    "linkify-it": "^5.0.0",
    "mermaid": "^11.6.0",
    "github-slugger": "^2.0.0",
    "@langchain/textsplitters": "^0.1.0",
    "@octokit/rest": "^21.1.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.5",
    "astro": "5.5.2",
    "astro-embed": "^0.9.0",
    "astro-font": "^1.0.0",
    "astro-i18n-aut": "^0.7.3",
    "cheerio": "1.0.0",
    "faiss-node": "^0.5.1",
    "ky": "^1.7.5",
    "langchain": "^0.3.19",
    "libnpmsearch": "^9.0.0",
    "marked": "^7.0.5",
    "openai": "^4.86.2",
    "astro-heroicons": "^2.1.5-1",
    "posthog-js": "^1.223.3",
    "sitemap": "^8.0.0",
    "striptags": "^3.2.0",
    "tailwindcss": "^4.1.5",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "seobot": "^1.3.0",
    "@antfu/eslint-config": "^4.3.0",
    "@astrojs/check": "^0.9.4",
    "@iconify-json/carbon": "^1.2.7",
    "@iconify-json/twemoji": "^1.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^6.0.0",
    "@types/node": "^22.13.0",
    "@types/semver": "^7.5.8",
    "@types/toastify-js": "^1.12.4",
    "dayjs": "^1.11.13",
    "eslint": "^9.21.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "nanospinner": "^1.2.2",
    "prettier": "^3.5.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "zod": "^3.24.2",
    "@inlang/paraglide-js": "2.0.12"
  }
}
